<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:textSize = "2dp" >
    <TextView
        android:id="@+id/tvpainMorn"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:textSize="20dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Morning Pain " />
    <TextView
        android:id="@+id/tvpainMid"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:textSize="20dp"

        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Midday Pain " />
    <TextView
    android:id="@+id/tvpainNight"
    android:layout_width="0dp"
    android:layout_weight="1"
    android:textSize="20dp"

    android:layout_height="wrap_content"
    android:gravity="center"
    android:text="Night Pain " />

    <TextView
        android:id="@+id/tvsleepTime"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:textSize="20dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Sleep Time " />

    <TextView
        android:id="@+id/tvsleepLength"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:textSize="20dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Sleep Length " />
</LinearLayout>
